  "keywords": [
    "forgeScript",
    "moderation",
    "feature-rich",
    "entertainment",
    "management",
    "automation",
    "utility",
    "multi-purpose",
    "discord-bot",
    "logging",
    "security",
    "fun",
    "customization",
    "protection",
    "commands",
    "bot-management",
    "server-tools",
    "community",
    "anti-spam",
    "advanced-features"
  ],
  "author": {
    "name": "Striatp",
    "email": "striatp@outlook.com",
    "url": "https://github.com/striatp"
  },
  "contributors": [],
  "license": "CC BY-NC-ND 4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaviumLabs/Pixxle.git"
  },
  "bugs": {
    "url": "https://github.com/RaviumLabs/Pixxle/issues"
  },
  "homepage": "https://ravium.org/pixxle",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/Ravium"
  },
  "engines": {
    "node": ">=23.10.0"
  },
  "engineStrict": true,
  "devDependencies": {
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "eslint": "^8.50.0"
  },
  "dependencies": {
    "@tryforge/forge.api": "^1.0.1",
    "@tryforge/forge.canvas": "github:tryforge/ForgeCanvas#dev",
    "@tryforge/forge.db": "github:tryforge/ForgeDB#dev",
    "@tryforge/forgescript": "github:tryforge/ForgeScript#dev",
    "dotenv": "^16.4.7",
    "path": "^0.12.7",
    "sqlite3": "^5.1.7"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "sqlite3"
